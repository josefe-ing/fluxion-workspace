{
  "name": "fluxion-dashboard-demo",
  "version": "1.0.0",
  "description": "Fluxion AI Dashboard Demo for Venezuelan B2B Wholesale Distribution",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "tsc && vite build",
    "build-single": "tsc && vite build --config vite.single.config.ts",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 50",
    "type-check": "tsc --noEmit",
    "db:setup": "node backend/scripts/setup-database.cjs init",
    "db:test": "node backend/scripts/setup-database.cjs test",
    "db:stats": "node backend/scripts/setup-database.cjs stats",
    "db:reset": "node backend/scripts/setup-database.cjs reset",
    "backend": "node server.cjs"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@sentry/react": "^10.21.0",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.294.0",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.3",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@types/pg": "^8.15.5",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "express": "^5.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "typescript": "^5.7.3",
    "vite": "^6.2.2",
    "vite-plugin-singlefile": "^2.0.3"
  }
}
