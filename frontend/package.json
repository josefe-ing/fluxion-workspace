{
  "name": "fluxion-dashboard-demo",
  "version": "1.0.0",
  "description": "Fluxion AI Dashboard Demo for Venezuelan B2B Wholesale Distribution",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-single": "tsc && vite build --config vite.single.config.ts",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "db:setup": "node backend/scripts/setup-database.cjs init",
    "db:test": "node backend/scripts/setup-database.cjs test",
    "db:stats": "node backend/scripts/setup-database.cjs stats",
    "db:reset": "node backend/scripts/setup-database.cjs reset",
    "backend": "node server.cjs"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.294.0",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@types/pg": "^8.15.5",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "express": "^5.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "terser": "^5.43.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-singlefile": "^0.13.5"
  }
}
