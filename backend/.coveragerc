[run]
source = .
omit =
    */venv/*
    */tests/*
    */conftest.py
    start.py
    simple_api.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

precision = 2

[html]
directory = htmlcov
